{
    "name": "@RideSaver/api",
    "version": "0.0.1",
    "description": "The RideSaver API client for Node.js and the browser.",
    "main": "build/js/index.js",
    "files": [
        "package.json",
        "build/js",
        "openapi.yaml",
        "README.md",
        "LICENSE"
    ],
    "author": {
        "name": "Elias Schablowski",
        "email": "eschabl@calstatela.edu"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RideSaver/API"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com"
    },
    "scripts": {
        "prebuild": "openapi -i openapi.yaml -o build/ts -c axios",
        "build": "npx @rtk-query/codegen-openapi ./redux-toolkit/openapi-config.ts",
        "postbuild": "tsc"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.0",
        "axios": "^1.1.3",
        "form-data": "^4.0.0"
    },
    "devDependencies": {
        "@rtk-query/codegen-openapi": "^1.0.0",
        "@types/node": "^18.11.9",
        "openapi-typescript-codegen": "^0.23.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "optionalDependencies": {
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "redux": "^4.2.0"
    }
}
